{
  "name": "cutter-sanborn-js",
  "version": "1.1.0",
  "description": "A library to calculate the call number based on the Cutter-Sanborn table",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "scrap": "ts-node prebuild/scrapper.ts",
    "parse": "ts-node prebuild/parser.ts",
    "transform": "npm run scrap && npm run parse && ts-node transform.ts",
    "build": "npm run transform && tsc",
    "build:cdn": "npm run transform && webpack",
    "prepublishOnly": "npm run build && npm run build:cdn && npm test",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "cutter",
    "sanborn",
    "library",
    "call number"
  ],
  "author": "Cleydyr de Albuquerque",
  "license": "ISC",
  "devDependencies": {
    "@types/functional-red-black-tree": "^1.0.6",
    "@types/jest": "^29.5.11",
    "cheerio": "^1.0.0",
    "jest": "^29.7.0",
    "json-loader": "^0.5.7",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "functional-red-black-tree": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cleydyr/cutter-sanborn-js.git"
  }
}